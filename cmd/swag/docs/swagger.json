            "properties": {
                "dateend": {
                    "type": "string",
                    "example": "2025-03-30 11:22:30"
                },
                "datestart": {
                    "type": "string",
                    "example": "2025-03-28 11:22:30"
                },
                "role_id": {
                    "type": "string",
                    "example": "2b796313-1134-44b3-b527-2c27d41a1624"
                }
            }
        },
        "model.LoginInput": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string",
                    "example": "thaco@1234"
                },
                "username": {
                    "type": "string",
                    "example": "Admin"
                }
            }
        },
        "model.MenuInput": {
            "type": "object",
