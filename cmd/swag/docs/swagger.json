                },
                "menu_group_name": {
                    "type": "string",
                    "example": "/api/dashboard"
                },
                "menu_icon": {
                    "type": "string",
                    "example": "database"
                },
                "menu_level": {
                    "type": "integer",
                    "example": 1
                },
                "menu_name": {
                    "type": "string",
                    "example": "Dashboard"
                },
                "menu_number_order": {
                    "type": "integer",
                    "example": 1
                },
                "menu_parent_id": {
                    "type": "string"
                },
                "menu_url": {
                    "type": "string",
                    "example": "/dashboard"
                }
            }
        },
